{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "*": ["*", "src/*"]
    },
    "outDir": "dist",
    "sourceMap": true
  },
  "include": [
    "src",
    "src/Assets/logo.svg",
    "./webpack.config.js",
    "./custom.d.ts"
  ],
  "exclude": ["node_modules", "dist"]
}
